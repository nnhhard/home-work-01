<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id='answerQuestionDao' class="ru.iashinme.dao.AnswerQuestionDaoImpl">
        <constructor-arg name="resourceName" value="answer-question.csv"/>
    </bean>

    <bean id='inputOutputService' class="ru.iashinme.service.InputOutputServiceImpl"/>

    <bean id='answerQuestionService' class="ru.iashinme.service.AnswerQuestionServiceImpl">
        <constructor-arg name="answerQuestionDao" ref="answerQuestionDao"/>
        <constructor-arg name="inputOutputService" ref="inputOutputService"/>
    </bean>


</beans>
